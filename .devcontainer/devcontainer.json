{
  "name": "Typescript",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "features": {
    "docker-in-docker": {
      "version": "latest",
      "moby": true
    },
    "azure-cli": "latest",
    "ghcr.io/devcontainers/features/github-cli:1": {},
    "ghcr.io/devcontainers/features/node:1": {}
  },

  "remoteUser": "vscode",
  "customizations": {
    "vscode": {
      "settings": {}
    },
    "extensions": [
      "github.copilot",
      "dbaeumer.vscode-eslint",
      "esbenp.prettier-vscode"
    ]
  }
}
